        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Age should be Between 21 - 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckedRegiDate</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND( ISBLANK(Resignation_Date__c)  ,
Is_on_Notice_Period__c), true, false)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Resignation Date Can not be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOBPlus18</fullName>
        <active>false</active>
        <errorConditionFormula>IF(YEAR(TODAY()) - YEAR( Date_of_Birth__c )&lt;18, true,false)</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IsOnNoticePeriod</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Is_on_Notice_Period__c  = false &amp;&amp;  ! ISBLANK( Resignation_Date__c) , true , false)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Not Allowed to Enter Registration Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Length</fullName>
        <active>true</active>
        <errorConditionFormula>IF( LEN( TEXT( SSN__c)) &gt; 10 , true, false)</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Length Cant be Greater than 10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Working_Day</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Last_Working_Day__c  &lt;  Date_of_Joining__c, true, false)</errorConditionFormula>
        <errorMessage>Enter Correct Last Working Day.</errorMessage>
    </validationRules>
</CustomObject>
